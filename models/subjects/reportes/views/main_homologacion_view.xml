<odoo>
    <data>
        <!-- Vista de formulario para tu modelo -->
        <record id="view_main_homologacion_form" model="ir.ui.view">
            <field name="name">main.homologacion.form</field>
            <field name="model">dara_mallas.main_homologacion</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="period_id"/>
                    </group>

                    <footer>
                        <button name="action_execute_code" string="Buscar" type="object" class="btn-primary"/>
                        <button name="update_file_links" string="Actualizar Lista" type="object" class="btn-secondary"/>
                    </footer>
                    
                    <div style="margin-bottom: 30px;"></div>

                    <group>
                        <field name="file_links" widget="html" force_save="1"/>                        
                    </group>
                </form>
            </field>
        </record>
        <!-- AcciÃ³n para la vista de formulario -->
        <record id="action_main_homologacion" model="ir.actions.act_window">
            <field name="name">Main Homologacion</field>
            <field name="res_model">dara_mallas.main_homologacion</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_main_homologacion_form"/>
        </record>
        <menuitem id="data_homologation_menu" 
                name="PRUEBA SCRIPT" 
                parent="dara_homologations_reports" 
                action="action_main_homologacion" 
                sequence="1"/>
    </data>
</odoo>
